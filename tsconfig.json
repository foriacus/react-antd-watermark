{
  "compilerOptions": {
    "target": "esnext",
    "moduleResolution": "node",
    "baseUrl": "./",
    "jsx": "preserve",
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@@/*": [
        "src/.umi/*"
      ],
      "antd-watermark": [
        "src/index.tsx"
      ]
    }
  },
  "include": [
    ".dumirc.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}